{
  "hosting": {
    "public": "./app/build",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "/resume",
        "function": "resume"
      },
      {
        "source": "/updateResume",
        "function": "updateResume"
      },
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  }
}
